<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="emptyForm" type="com.sunyard.hgam.presentation.base.BaseForm" />
  </form-beans>
  <global-forwards>
    <forward name="failure" path="/error.jsp" redirect="true" />
    <forward name="unknown-error" path="/error.jsp" />
    <forward name="global-login" path="/login.htm" redirect="true" />
  </global-forwards>
  <controller nocache="true" locale="false" contentType="text/html; charset=GBK" />
  <message-resources parameter="properties.struts.resources.ApplicationResources" />
</struts-config>
